{
  "name": "react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "ayush": "npm install && tsx src/console/ayush.ts && vite --open",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "clean": "rm -rf node_modules && rm -rf package-lock.json && rm -rf dist && npm cache clean --force && rm -rf .vercel",
    "cleanup": "npx modclean --run",
    "typecheck": "tsc --noEmit",
    "directory": "find . -type d -empty -delete",
    "deploy": " vercel --prod --force"
  },
  "dependencies": {
    "@cyntler/react-doc-viewer": "^1.17.1",
    "@tailwindcss/vite": "^4.1.17",
    "docx-preview": "^0.3.7",
    "katex": "^0.16.21",
    "lucide-react": "^0.553.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-medium-image-zoom": "^5.4.0",
    "react-pdf": "^10.2.0",
    "react-router-dom": "^7.9.5",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react-swc": "^4.2.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.2.2"
  }
}
